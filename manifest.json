{
  "name": "Custom Zoom Increments",
  "description": "Zoom in at custom increments",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "zoom-in": {
      "suggested_key": {
        "default": "Alt+8"
      },
      "description": "Zoom out by the specified increment",
      "global": true
    },
    "zoom-out": {
      "suggested_key": {
        "default": "Alt+9"
      },
      "description": "Zoom in by the specified increment",
      "global": true
    },
    "zoom-default": {
      "suggested_key": {
        "default": "Alt+0"
      },
      "description": "Zoom to the specified value",
      "global": true
    }
  }
}
